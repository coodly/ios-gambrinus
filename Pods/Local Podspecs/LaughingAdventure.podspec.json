        "CoreDataPersistence": [
          "0.1.8"
        ]
      }
    },
    {
      "name": "Logging",
      "source_files": "Source/Log"
    },
    {
      "name": "Utils",
      "source_files": "Source/Utils"
    },
    {
      "name": "Purchase",
      "dependencies": {
        "LaughingAdventure/Logging": [

        ]
      },
      "source_files": "Source/Purchase",
      "frameworks": "StoreKit"
    },
    {
      "name": "Feedback",
      "source_files": "Source/Feedback",
      "dependencies": {
        "CoreDataPersistence": [
          "0.1.8"
        ]
      },
      "frameworks": "CoreData"
    },
    {
      "name": "Cloud",
      "dependencies": {
        "LaughingAdventure/Logging": [

        ],
        "LaughingAdventure/Utils": [

        ]
      },
      "source_files": "Source/Cloud"
    }
  ]
}
